{
    "name": "@eden-network/network-subgraph",
    "license": "MIT",
    "version": "1.0.0",
    "author": "Jeffrey Quesnelle <jeff@edennetwork.io>",
    "scripts": {
        "codegen": "graph codegen subgraph.yaml",
        "build": "graph build subgraph.yaml",
        "create-local": "dotenv -e .env -- cross-var graph create --node %GRAPH_NODE% eden-distributor/network",
        "remove-local": "dotenv -e .env -- cross-var graph remove --node %GRAPH_NODE% eden-distributor/network",
        "deploy-local": "dotenv -e .env -- cross-var graph deploy --node %GRAPH_NODE% --ipfs %IPFS_NODE% eden-distributor/network",
        "deploy-hosted:ropsten": "dotenv -e .env -- cross-var graph deploy --product hosted-service jquesnelle/network",
        "deploy-hosted:mainnet": "dotenv -e .env -- cross-var graph deploy --product hosted-service eden-network/network",
        "prepare:ropsten": "mustache config/ropsten.json subgraph.template.yaml > subgraph.yaml",
        "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml"
    },
    "devDependencies": {
        "@graphprotocol/graph-cli": "^0.21.0",
        "@graphprotocol/graph-ts": "^0.20.1",
        "cross-var": "^1.1.0",
        "dotenv-cli": "^4.0.0",
        "mustache": "^4.2.0",
        "typescript": "^4.3.4"
    }
}
